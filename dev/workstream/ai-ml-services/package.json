{
  "name": "@spark-den/ai-ml-services",
  "version": "1.0.0",
  "description": "AI/ML Services workstream for healthcare coding and document processing",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc --build",
    "test": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "vitest run tests/e2e",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "lint": "eslint src tests --ext .ts,.tsx",
    "lint:fix": "eslint src tests --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "serve": "node dist/index.js",
    "validate:bias": "tsx scripts/validate-bias.ts",
    "validate:accuracy": "tsx scripts/validate-accuracy.ts",
    "compliance:check": "tsx scripts/compliance-check.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "winston": "^3.11.0",
    "node-cron": "^3.0.3",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.2",
    "tsx": "^4.6.0",
    "vitest": "^1.0.4",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1"
  },
  "keywords": [
    "ai",
    "ml",
    "healthcare",
    "medical-coding",
    "document-processing",
    "computer-vision",
    "bias-detection",
    "hipaa-compliance"
  ],
  "author": "Spark Den AI/ML Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}