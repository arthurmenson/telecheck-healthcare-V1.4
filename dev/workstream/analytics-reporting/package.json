  "description": "TDD-driven analytics and reporting workstream with real-time dashboards and predictive analytics",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc && vite build",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "start": "node dist/server.js",
    "analytics:validate": "tsx src/validation/dashboard-validator.ts",
    "analytics:benchmark": "tsx src/benchmarks/performance-benchmark.ts",
    "data:quality": "tsx src/data-quality/pipeline-validator.ts",
    "ml:train": "tsx src/ml/model-trainer.ts",
    "streaming:start": "tsx src/streaming/analytics-processor.ts",
    "health:analytics": "tsx src/health/population-analytics.ts",
    "financial:report": "tsx src/financial/revenue-tracker.ts"
