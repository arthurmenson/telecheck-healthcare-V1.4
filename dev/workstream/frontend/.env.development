# Development Environment Variables for Telecheck Health Frontend

# API Configuration - Connect to running backend services
VITE_API_URL=http://localhost:3001/api

# Service URLs for different workstreams
VITE_AI_ML_API_URL=http://localhost:3000/api
VITE_PMS_CORE_API_URL=http://localhost:3001/api
VITE_ANALYTICS_API_URL=http://localhost:3003/api
VITE_INTEGRATIONS_API_URL=http://localhost:3004/api

# Authentication & Security
VITE_JWT_SECRET=dev_jwt_secret_key
VITE_ENCRYPTION_KEY=dev_encryption_key

# Feature Flags
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_BILLING=true
VITE_ENABLE_INTEGRATIONS=true

# Development Settings
VITE_DEBUG_MODE=true
VITE_MOCK_DATA=false
VITE_LOG_LEVEL=debug

# Healthcare Compliance
VITE_HIPAA_MODE=true
VITE_AUDIT_LOGGING=true

# Payment Processing
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_healthcare_demo_key
VITE_PAYMENT_ENVIRONMENT=test

# Monitoring & Analytics
VITE_MONITORING_ENABLED=true
VITE_PERFORMANCE_TRACKING=true