{
  "name": "auth-security-workstream",
  "version": "1.0.0",
  "description": "Enterprise-grade authentication and security with JWT, RBAC, and HIPAA compliance",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:security": "jest --testPathPattern=security",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "security:audit": "npm audit",
    "security:scan": "snyk test"
  },
  "keywords": [
    "authentication",
    "security",
    "jwt",
    "rbac",
    "hipaa",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mjml": "^4.7.4",
    "@types/node": "^20.11.24",
    "@types/nodemailer": "^7.0.1",
    "@types/pg": "^8.11.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "snyk": "^1.1291.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.29.4",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "mjml": "^4.15.3",
    "mjml-core": "^4.15.3",
    "nodemailer": "^7.0.6",
    "pg": "^8.11.3",
    "redis": "^4.6.13",
    "uuid": "^9.0.1",
    "winston": "^3.12.0"
  },
  "directories": {
    "test": "tests"
  },
  "type": "commonjs"
}
