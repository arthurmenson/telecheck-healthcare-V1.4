{
    "containerDefinitions": [
        {
            "name": "ai-ml-service",
            "image": "901076063012.dkr.ecr.us-east-1.amazonaws.com/spark-den-ai-ml-services:67fbabf6bacb4e5f16a3b063617eaa0092ceb9c5",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "REDIS_HOST",
                    "value": "spark-den-redis.l71oy0.0001.use1.cache.amazonaws.com"
                },
                {
                    "name": "PORT",
                    "value": "3000"
                },
                {
                    "name": "REDIS_PORT",
                    "value": "6379"
                },
                {
                    "name": "DB_PORT",
                    "value": "5432"
                },
                {
                    "name": "DB_USER",
                    "value": "postgres"
                },
                {
                    "name": "NODE_ENV",
                    "value": "production"
                },
                {
                    "name": "DB_NAME",
                    "value": "sparkden"
                },
                {
                    "name": "DB_HOST",
                    "value": "spark-den-db.ckl6gs44yvky.us-east-1.rds.amazonaws.com"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "secrets": [
                {
                    "name": "DB_PASSWORD",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:901076063012:secret:spark-den-db-password-aV8lGH"
                },
                {
                    "name": "JWT_SECRET",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:901076063012:secret:spark-den-jwt-secret-mT4zfH"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/spark-den/ai-ml-service",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://localhost:3000/health || exit 1"
                ],
                "interval": 30,
                "timeout": 5,
                "retries": 3,
                "startPeriod": 60
            },
            "systemControls": []
        }
    ],
    "family": "spark-den-ai-ml-service",
    "taskRoleArn": "arn:aws:iam::901076063012:role/spark-den-ecs-task-role",
    "executionRoleArn": "arn:aws:iam::901076063012:role/spark-den-ecs-task-execution-role",
    "networkMode": "awsvpc",
    "volumes": [],
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "1024",
    "memory": "2048"
}
